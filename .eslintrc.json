{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "next/core-web-vitals",
        "eslint:recommended",
        "next",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/function-component-definition": "off",
        "import/prefer-default-export": "off",
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "endOfLine": "auto",
                "printWidth": 140
            }
        ],
        "camelcase": "off"
    }
}